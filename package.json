{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --minify",
    "dev": "concurrently \"npm run dev:css\" \"node -r dotenv/config node_modules/.bin/remix dev\"",
    "dev:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --watch",
    "postinstall": "remix setup node",
    "start": "remix-serve build",
    "cy": "cypress open"
  },
  "dependencies": {
    "@prisma/client": "^3.11.0",
    "@remix-run/react": "^1.3.2",
    "@remix-run/serve": "^1.3.2",
    "@remix-run/vercel": "^1.3.2",
    "@supabase/supabase-js": "^1.31.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "framer-motion": "^6.2.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.3.2",
    "remix-auth": "^3.2.1",
    "remix-auth-supabase": "^3.1.0",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.3.2",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.0.0",
    "cypress": "^9.5.2",
    "postcss": "^8.4.12",
    "prisma": "^3.11.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
